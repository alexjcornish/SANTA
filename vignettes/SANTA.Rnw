# for (name in network.names) {
#     results[[name]] <- Knet(networks[[name]], nperm=1000, 
#         vertex.attr=go.terms, edge.attr="distance")
#     results[[name]] <- sapply(results[[name]], function(res) res$pval)
# }
@

This code can be used to plot the results. 

<<echo=true>>= 
